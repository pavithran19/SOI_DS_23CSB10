function palindrome(word){
    let len = word.length;
    for (let i = 0; i <= len; i++) {
        if(word[i]!==word[len-1-i])
        {
            console.log("Not Palindrome");
            return;
        }
        
    }
    console.log("Palindrome");
}
palindrome("MADAM");
palindrome("HELLO");
